{
  "name": "gatsby-starter-typescirpt-power-blog",
  "description": "gatsyby blog template with typescript and tslint",
  "version": "1.1.0",
  "author": "Majid Hajian ( mhadaily@gmail.com )",
  "keywords": [
    "gatsby, typescript",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build",
    "start": "gatsby clean && gatsby develop",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:netlify": "gatsby build --prefix-paths && netlify deploy --prod --dir=public",
    "develop": "gatsby develop -H 0.0.0.0 -p 8082",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "lint:ts": "tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "lint:style": "stylelint 'src/**/*.{scss,ts,css,sass,less,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,md}'",
    "formatAndFix": "npm run format && npm run lint:ts && npm run style"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "@types/typography": "^0.16.4",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby-cli": "^4.16.0",
    "gatsby-plugin-catch-links": "^4.16.0",
    "gatsby-plugin-lodash": "^5.16.0",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-netlify": "^5.0.0",
    "gatsby-plugin-offline": "^5.16.0",
    "gatsby-plugin-purgecss": "^6.1.2",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-sass": "^5.16.0",
    "gatsby-plugin-sharp": "^4.16.1",
    "gatsby-plugin-sitemap": "^5.16.0",
    "gatsby-plugin-styled-components": "^5.16.0",
    "gatsby-plugin-typescript": "^4.16.0",
    "gatsby-plugin-typography": "^4.16.0",
    "gatsby-remark-autolink-headers": "^5.16.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^6.16.0",
    "gatsby-source-filesystem": "^4.16.0",
    "gatsby-transformer-remark": "^5.16.0",
    "gatsby-transformer-sharp": "^4.16.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-typography": "^0.16.20",
    "source-map-support": "latest",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.8.1",
    "tslint": "^6.1.3",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "tslint-react-a11y": "^1.1.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "gatsby": "^4.16.0",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-google-tagmanager": "^4.16.0",
    "lodash": "^4.17.21",
    "polished": "^4.2.2",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.5",
    "typography": "^0.16.21"
  }
}
